{
  "name": "@falkordb/langchain-ts",
  "version": "0.1.0",
  "description": "FalkorDB integration for LangChain.js - A blazing fast graph database for AI applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:int": "jest --testMatch='**/*.int.test.ts'",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "langchain",
    "falkordb",
    "graph",
    "graph-database",
    "database",
    "ai",
    "llm",
    "cypher",
    "knowledge-graph",
    "redis"
  ],
  "author": "FalkorDB Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FalkorDB/FalkorDB-Langchain-js.git"
  },
  "bugs": {
    "url": "https://github.com/FalkorDB/FalkorDB-Langchain-js/issues"
  },
  "homepage": "https://github.com/FalkorDB/FalkorDB-Langchain-js#readme",
  "peerDependencies": {
    "@langchain/core": ">=0.1.0 <2.0.0",
    "langchain": ">=0.1.0"
  },
  "dependencies": {
    "falkordb": "^6.3.0"
  },
  "devDependencies": {
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "langchain": "^0.3.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
